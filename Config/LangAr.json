"battlefield_build_name_01" : "\tالهرم الأكبر\t",
"battlefield_build_name_02" : "\tالهرم الأصغر\t",
"battlefield_build_name_03" : "\tنقطة التموين\t",
"battlefield_build_name_04" : "\tنقطة الانتقال\t",
